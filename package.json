{
  "name": "@jl-org/tool",
  "version": "3.4.0",
  "description": "JS的瑞士军刀",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "type": "module",
  "types": "dist/typings/index.d.ts",
  "files": [
    "/dist"
  ],
  "exports": {
    ".": {
      "require": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./node": {
      "require": {
        "types": "./dist/node/typings/node/index.d.ts",
        "default": "./dist/node/index.cjs"
      },
      "import": {
        "types": "./dist/node/typings/node/index.d.ts",
        "default": "./dist/node/index.js"
      }
    }
  },
  "typesVersions": {
    "*": {
      "node": [
        "./dist/node/typings/node/index.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "run-p build:only",
    "build:only": "rollup -c && tsc-alias",
    "prepublishOnly": "npm run build",
    "------doc------": "",
    "gen-doc": "typedoc --tsconfig tsconfig.json",
    "docs:dev": "vitepress dev ./docs",
    "docs:build": "vitepress build ./docs",
    "docs:preview": "vitepress preview ./docs",
    "------test------": "",
    "test": "vitest --run",
    "test:dom": "vite",
    "lint": "eslint --fix src"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.13.2",
    "@jl-org/eslint-plugins": "^1.0.0",
    "@jl-org/ts-tool": "^0.0.8",
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/node": "^20.11.17",
    "@types/webspeechapi": "^0.0.29",
    "@types/wicg-file-system-access": "^2023.10.6",
    "@vitest/coverage-v8": "^2.1.8",
    "eslint": "^9.39.2",
    "jsdom": "^25.0.1",
    "npm-run-all2": "^8.0.2",
    "rollup": "^4.6.1",
    "rollup-plugin-clear": "^2.0.7",
    "tsc-alias": "^1.8.8",
    "tslib": "^2.6.2",
    "typedoc": "^0.25.10",
    "typedoc-plugin-markdown": "^3.17.1",
    "vite": "^6.0.3",
    "vitepress": "1.3.1",
    "vitest": "^2.1.8"
  },
  "keywords": [
    "tool",
    "jl-tool"
  ],
  "author": "CJL <2662442385@qq.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/beixiyo/jl-tool"
}
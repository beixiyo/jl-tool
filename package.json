{
    "name": "@jl-org/tool",
    "version": "1.1.93",
    "description": "TypeScript编写的工具函数",
    "main": "dist/index.cjs",
    "module": "dist/index.js",
    "type": "module",
    "types": "dist/typings/index.d.ts",
    "files": [
        "/dist"
    ],
    "exports": {
        "require": {
            "types": "./dist/typings/index.d.ts",
            "default": "./dist/index.cjs"
        },
        "import": {
            "types": "./dist/typings/index.d.ts",
            "default": "./dist/index.js"
        }
    },
    "scripts": {
        "build": "npm-run-all --parallel build:only gen-doc",
        "build:only": "rollup -c && tsc-alias",
        "gen-doc": "typedoc --tsconfig tsconfig.json",
        "preview-doc": "vitepress dev ./doc --open",
        "build-doc": "vitepress build ./doc",
        "test": "pnpm -C test dev"
    },
    "keywords": [
        "jl",
        "tool",
        "tools",
        "jltool",
        "jltools",
        "jl-tool",
        "jl-tools"
    ],
    "author": "cjl",
    "license": "MIT",
    "devDependencies": {
        "@rollup/plugin-alias": "^5.1.0",
        "@rollup/plugin-node-resolve": "^15.2.3",
        "@rollup/plugin-terser": "^0.4.4",
        "@rollup/plugin-typescript": "^11.1.5",
        "@types/node": "^20.11.17",
        "@types/webspeechapi": "^0.0.29",
        "npm-run-all": "^4.1.5",
        "rollup": "^4.6.1",
        "rollup-plugin-clear": "^2.0.7",
        "tsc-alias": "^1.8.8",
        "tslib": "^2.6.2",
        "typedoc": "^0.25.10",
        "typedoc-plugin-markdown": "^3.17.1",
        "typescript": "^5.3.3",
        "vitepress": "1.0.0-rc.44"
    },
    "publishConfig": {
        "access": "public"
    }
}